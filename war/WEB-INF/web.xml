<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>KyoryuIce</servlet-name>
		<servlet-class>jp.ac.tsuda.kyoryuIce.KyoryuIceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KyoryuIce</servlet-name>
		<url-pattern>/kyoryuice</url-pattern>		
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>add_data</servlet-name>
    	<servlet-class>jp.ac.tsuda.kyoryuIce.AddServlet</servlet-class>
    </servlet>
	<servlet-mapping>
    	<servlet-name>add_data</servlet-name>
    	<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
